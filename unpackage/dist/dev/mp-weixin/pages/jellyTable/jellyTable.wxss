.atom {
  text-decoration: none;
  position: relative;
  overflow: hidden;
}
.atom:hover {
  box-shadow: 1px 1px 25px 10px rgba(146, 148, 248, 0.4);
}
.atom:before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: -webkit-linear-gradient(330deg, transparent, rgba(146, 148, 248, 0.4), transparent);
  background: linear-gradient(120deg, transparent, rgba(146, 148, 248, 0.4), transparent);
  -webkit-transition: all 650ms;
  transition: all 650ms;
}
.atom:hover:before {
  left: 100%;
}
