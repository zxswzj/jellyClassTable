/*========== flex布局相关 ==========*/
.dp-f {
  display: -webkit-box;
  display: flex;
  display: -webkit-flex;/*在webkit内核的浏览器上使用要加前缀*/
}
/*快捷水平垂直居中*/
.dp-fc {
  display: -webkit-box;
  display: flex;
  display: -webkit-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
/*行内块元素*/
.dp-ib {
  display: inline-block;
}
/* 主轴方向 */
.fd-rr {
	-webkit-box-orient:horizontal;
	-webkit-box-direction:reverse;
	-webkit-flex-direction:row-reverse;
	        flex-direction:row-reverse;/* 主轴方向从右到左,默认从左到右 */
}
.fd-c {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;/* 主轴方向从上到下,默认从左到右 */
}
.fd-cr {
  -webkit-box-orient: vertical;
  -webkit-box-direction: reverse;
  -webkit-flex-direction: column-reverse;
          flex-direction: column-reverse;/* 主轴方向从下到上,默认从左到右 */
}
/* flex-wrap属性 */
.fw-w {
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;/* 换行，第一行在上方。 */
}
.fw-wr{
	-webkit-flex-wrap: wrap-reverse;
	        flex-wrap: wrap-reverse;/* 换行，第一行在下方。 */
}
/* justify-content属性 */
.jc-fs {
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;/* 在主轴左对齐-默认 */
}
.jc-fe {
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;/* 在主轴右对齐 */
}
.jc-c {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center; /* 在主轴居中对齐 */
}
.jc-sb {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;/* 两端对齐（两边不留空） */
}
.jc-sa {
  -webkit-justify-content: space-around;
          justify-content: space-around;/* 等距对齐*/
}
/* align-items属性 */
.ai-fs {
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
}
.ai-fe {
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;/* 底对齐 */
}
.ai-c {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;/* 水平居中 */
}
.ai-bl {
  -webkit-box-align: baseline;
  -webkit-align-items: baseline;
          align-items: baseline;/* 水平居中 */
}
/* align-content属性(多列对齐) */
.ac-fs{
  -webkit-align-content:flex-start;
          align-content:flex-start;/* 与交叉轴的起点对齐 */
}
.ac-fe{
  -webkit-align-content:flex-end;
          align-content:flex-end;/* 与交叉轴的终点对齐 */
}
.ac-c {
  -webkit-align-content: center;
          align-content: center; /* 与交叉轴的中点对齐。 */
}
.ac-sb{
  -webkit-align-content:space-between;
          align-content:space-between;/* 与交叉轴两端对齐，轴线之间的间隔平均分布。 */
}
.ac-sa{
  -webkit-align-content:space-around;
          align-content:space-around;/* 每根轴线两侧的间隔都相等。所以，轴线之间的间隔比轴线与边框的间隔大一倍。 */
}
/* align-self属性(默认auto),该属性用于子元素特殊处理 */
.as-fs {
  -webkit-align-self: flex-start;
          align-self: flex-start;/* 自身在交叉左对齐 */
}
.as-fe {
  -webkit-align-self: flex-end;
          align-self: flex-end;/* 自身在交叉右对齐 */
}
.as-c {
  -webkit-align-self: center;
          align-self: center;/* 自身在交叉居中对齐 */
}
.as-bl {
  -webkit-align-self: baseline;
          align-self: baseline;/* 自身根据文字对齐 */
}
